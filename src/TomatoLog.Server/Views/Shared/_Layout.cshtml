<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - TomatoLog.Server</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <ul class="nav bg-dark">
        <li>
            <a asp-area="" asp-controller="Home" asp-action="Index" class="navbar-brand">TomatoLog.Server</a>
        </li>
        <li class="nav-item"><a class="nav-link" asp-area="" asp-controller="Home" asp-action="Index">Home</a></li>
        <li class="nav-item"><a class="nav-link" asp-area="" asp-controller="Config" asp-action="Index">Config</a></li>
        <li class="nav-item"><a class="nav-link" asp-area="" asp-controller="ProConfig" asp-action="Index">Project</a></li>
        <li class="nav-item"><a class="nav-link" asp-area="" asp-controller="Home" asp-action="Contact">Contact</a></li>
    </ul>
    <div class="">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; 2019 - TomatoLog.Server</p>
        </footer>
    </div>
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
    <script src="~/js/site.min.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
